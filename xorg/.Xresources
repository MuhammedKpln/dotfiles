Xft.antialias: 1
Xft.autohint: 0
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba: rgb

Xcursor.theme: Adwaita

dzen2.font: Misc Tamsyn Misc:size=6

URxvt.pointerBlank: true

URxvt.termName: rxvt-256color
URxvt.saveLines:65535
URxvt.internalBorder:3
URxvt.externalBorder:3
!URxvt.transparent:true
!URxvt.tintColor: #0c0c0c
!URxvt.blendType: alphablend
URxvt.letterSpace: 0
URxvt.depth: 32

URxvt.font: xft:spectrum berry:size=7, xft:Noto Sans Kannada
URxvt.boldFont: xft:spectrum berry::size=7, xft:Noto Sans Kannada
URxvt.italicFont: xft:spectrum berry:size=7, xft:Noto Sans Kannada
URxvt.boldItalicFont: xft:spectrum berry:size=7, xft:Noto Sans Kannada

URxvt.iso14755: false
URxvt.meta8: false
!URxvt.buffered: false
URxvt.eightBitInput: false
URxvt.metaSendsEscape: true
URxvt.modifier: alt

!URxvt.keysym.Escape: \033\007

URxvt.scrollBar:false
URxvt.scrollstyle:rxvt
URxvt.perl-lib: /usr/lib/urxvt/perl/
URxvt.perl-ext-common: default,matcher,clipboard,searchable-scrollback,url-select,keyboard-select
URxvt.urlLauncher: chromium
URxvt.matcher.rend.0: Uline fg14
!URxvt.matcher.pattern.1: (?:https?://|ftp://|news://|mailto:|file://|\bwww\.)[\w\-\@;\/?:&=%\$.+!*\x27,~#\n]*[^\s]
URxvt.cutchars: "\\ `\"\'()*;<>[]{|}"

URxvt.url-select.autocopy: true
!URxvt.url-select.button: 1
URxvt.url-select.launcher: firefox
URxvt.url-select.underline: true

URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true

URxvt.keysym.M-c: perl:keyboard-select:activate
URxvt.keysym.M-y: perl:clipboard:copy
URxvt.keysym.M-p: perl:clipboard:paste
URxvt.keysym.M-C-p: perl:clipboard:paste_escaped
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.M-u: perl:url-select:select_next

